{
  "name": "redibox",
  "description": "Redis connection and PUBSUB subscription manager for node. Built for performance, powered by ioredis.",
  "version": "2.3.0",
  "main": "lib/index.js",
  "author": "Mike Diarmid",
  "repository": "redibox/core",
  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "keywords": [
    "redis",
    "redis-cluster",
    "cluster",
    "scheduler",
    "scheduling",
    "waterline",
    "sails",
    "cache",
    "throttle",
    "throttling",
    "timeseries",
    "autocomplete",
    "indexes",
    "indices",
    "lock",
    "redlock",
    "lua",
    "ioredis"
  ],
  "scripts": {
    "example": "cd example && node app.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "node -- ./node_modules/.bin/babel-istanbul cover --report lcov _mocha "
