{
  "name": "redibox",
  "description": "Ultimate redis toolbox for node, featuring caching, clusters, cluster scaled slave reads, locks, throttling, timeseries, additional new commands (via lua scripts) and more features! Built in ES6/ES7",
  "version": "1.0.0",
  "main": "lib/index.js",
  "author": "Mike Diarmid",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "redis",
    "redis-cluster",
    "cluster",
    "scheduler",
    "scheduling",
    "waterline",
    "sails",
    "cache",
    "throttle",
    "throttling",
    "timeseries",
    "autocomplete",
    "indexes",
    "indices",
    "lock",
    "redlock",
    "lua",
    "ioredis"
  ],
  "scripts": {
    "compile": "NODE_ENV=production babel src --out-dir lib",
    "build": "babel src --out-dir lib",
    "build:watch": "babel src --watch --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha "
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-class-properties": "^6.8.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-syntax-function-bind": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015-node6": "^0.1.3",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bluebird": "^3.3.5",
    "cuid": "^1.3.8",
    "ioredis": "^2.0.0-rc2",
    "winston": "^2.2.0"
  }
}
