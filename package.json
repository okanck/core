  "devDependencies": {
    "benchmark": "^2.1.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "isparta": "^4.0.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "cuid": "^1.3.8",
    "eventemitter3": "^2.0.0",
    "ioredis": "^2.3.0",
    "winston": "^2.2.0"
  }
