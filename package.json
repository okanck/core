{
  "name": "redibox",
  "description": "Redis connection and PUBSUB subscription manager for node. Built for performance, powered by ioredis.",
  "version": "1.0.0",
  "main": "lib/index.js",
  "author": "Mike Diarmid",
  "repository": "redibox/core",
  "license": "MIT",
  "engines": {
    "node": ">=4.3"
  },
  "keywords": [
    "redis",
    "redis-cluster",
    "cluster",
    "scheduler",
    "scheduling",
    "waterline",
    "sails",
    "cache",
    "throttle",
    "throttling",
    "timeseries",
    "autocomplete",
    "indexes",
    "indices",
    "lock",
    "redlock",
    "lua",
    "ioredis"
  ],
  "scripts": {
    "build": "npm run buildX &&  npm run build4 && npm run build5 && npm run build6 && cp lib/x/index.js lib",
    "buildX": "babel src --babelrc=false --presets es2015,stage-0 --plugins add-module-exports --out-dir lib/X",
    "build4": "babel src --babelrc=false --presets es2015-node4,stage-0 --plugins add-module-exports --out-dir lib/4",
    "build5": "babel src --babelrc=false --presets es2015-node5,stage-0 --plugins add-module-exports --out-dir lib/5",
    "build6": "babel src --babelrc=false --presets es2015-node6,stage-0 --plugins add-module-exports --out-dir lib/6",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "./node_modules/.bin/babel-node -- ./node_modules/.bin/isparta cover _mocha "
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node-auto": "0.0.3",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-es2015-node6": "^0.1.3",
    "babel-preset-stage-0": "^6.5.0",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "bluebird": "^3.3.5",
    "cuid": "^1.3.8",
    "eventemitter3": "^1.2.0",
    "ioredis": "^2.0.0-rc3",
    "winston": "^2.2.0"
  }
}
