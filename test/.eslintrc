{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "after": true
  }
}
